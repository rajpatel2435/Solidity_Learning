pragma solidity ^0.8.0;

contract Type {
    // Basic Types:
    // bool, uint , int, address , bytes

    // boolean
    bool isFavourite = false;

    // unit256 is equal to uint256
    // usigned integer
    uint256 age = 25;
    // signed integer
    int256 minusNumber = -25;

    // string
    string name = "Raj";

    // address

    // address myMetaMask= 0x9f8F720x9f8F72;

    // bytes

    // notes if you just declare varibale in this it has some default value
    // integrer 0 and bool has false
}
